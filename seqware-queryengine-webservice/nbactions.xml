<?xml version="1.0" encoding="UTF-8"?>
<actions>
        <action>
            <actionName>run</actionName>
            <goals>
                <goal>process-classes</goal>
                <goal>org.codehaus.mojo:exec-maven-plugin:1.2.1:exec</goal>
            </goals>
            <properties>
                <exec.args>-classpath %classpath com.github.seqware.queryengine.system.exporters.VCFDumper 87a5d318-a4ad-4d05-918f-34702eb70c1d test_out.vcf</exec.args>
